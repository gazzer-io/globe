{
  "name": "@gazzer/globe",
  "version": "0.1.3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bsconfig.json",
    "src/**",
    "lib/**",
    "es/**"
  ],
  "browserslist": [
    "IE >= 11",
    "Firefox >= 60",
    "Safari >= 11.1",
    "Chrome >= 66",
    "ChromeAndroid >= 66",
    "iOS >= 11.3",
    "Edge >= 15"
  ],
  "scripts": {
    "dev": "now dev",
    "start": "next start",
    "export": "next export",
    "deploy": "yarn build && now --target production",
    "clean": "rimraf lib es coverage public/storybook public/playroom",
    "build": "yarn build:package && yarn build:page",
    "build:package": "yarn run clean && yarn build:elodin && yarn build:reason && yarn build:es && yarn build:lib",
    "build:page": "next build && yarn build:storybook && yarn build:playroom",
    "build:es": "babel src --out-dir es --ignore \"src/__tests__\" --copy-files",
    "build:lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore \"src/__tests__\" --copy-files",
    "build:gen": "cross-env BABEL_ENV=commonjs babel generator/src --out-dir generator/out --ignore \"src/__tests__\" --copy-files",
    "build:reason": "bsb -clean-world -make-world",
    "build:elodin": "elodin src --clean",
    "build:icons": "node scripts/icons.js && svgr -d src/icons/js src/icons/svg --icon",
    "build:storybook": "build-storybook -c .storybook -o ./public/storybook",
    "build:playroom": "playroom build",
    "test": "cross-env BABEL_ENV=commonjs echo 1",
    "coverage": "yarn test --coverage",
    "storybook": "start-storybook -p 6006 -s ./",
    "playroom": "playroom start",
    "reason": "bsb -clean-world -make-world -w",
    "release": "yarn build:package && npm publish",
    "elodin": "elodin src --watch --clean"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-bucklescript": "^0.5.3",
    "bs-moment": "^0.4.4",
    "bs-platform": "^5.0.6",
    "fela": "^10.8.2",
    "fela-preset-web": "^10.8.2",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dates": "^21.0.1",
    "react-dom": "^16.9.0",
    "react-fela": "^10.8.2",
    "reason-fela": "^10.8.2",
    "reason-react": "^0.7.0",
    "use-google-autocomplete": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@elodin/cli": "^0.0.41",
    "@elodin/core": "^0.0.41",
    "@elodin/generator-reason": "^0.0.41",
    "@elodin/parser": "^0.0.41",
    "@elodin/plugin-rename-variable": "^0.0.41",
    "@elodin/plugin-replace-variable": "^0.0.41",
    "@elodin/utils": "^0.0.41",
    "@next/bundle-analyzer": "^9.1.3",
    "@now/next": "^0.5.8",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@svgr/cli": "^4.3.2",
    "@zeit/next-css": "^1.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "dom-parser": "^0.1.6",
    "jest": "^23.6.0",
    "next": "^9.1.1",
    "next-offline": "^4.0.6",
    "next-transpile-modules": "^2.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "playroom": "^0.9.4",
    "prettier-plugin-elodin": "^0.0.41",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "webpack-dev-server": "^3.9.0",
    "xmldom": "^0.1.27"
  }
}
