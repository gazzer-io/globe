{
  "name": "@gazzer/globe",
  "namespace": "globe",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": ["reason-react", "reason-fela", "bs-moment"],
  "reason": {
    "react-jsx": 3
  },
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": ["-bs-super-errors"],
  "refmt": 3
}
