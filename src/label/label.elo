variant Status {
  Disabled
}

view Label {
  __WebkitUserSelect: none
  __MozUserSelect: none
  __userSelect: none
  __cursor: $cursor
  alignSelf: auto
  paddingLeft: 4
}

text LabelText {
  color: rgb(100 100 100)
  lineHeight: 1.1
  fontSize: 16
  [Status=Disabled] {
    color: rgb(150 150 150)
  }
}
