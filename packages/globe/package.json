{
  "name": "@gazzer/globe",
  "version": "0.1.4",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bsconfig.json",
    "src/**",
    "lib/**",
    "es/**"
  ],
  "browserslist": [
    "IE >= 11",
    "Firefox >= 60",
    "Safari >= 11.1",
    "Chrome >= 66",
    "ChromeAndroid >= 66",
    "iOS >= 11.3",
    "Edge >= 15"
  ],
  "scripts": {
    "clean": "rimraf lib es coverage",
    "build": "yarn build:icons && yarn build:package && yarn build:tools",
    "build:package": "yarn run clean && yarn build:elodin && yarn build:reason && yarn build:es && yarn build:lib",
    "build:tools": "yarn build:storybook && yarn build:playroom",
    "build:es": "babel src --out-dir es --ignore \"src/__tests__\" --copy-files",
    "build:lib": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore \"src/__tests__\" --copy-files",
    "build:gen": "cross-env BABEL_ENV=commonjs babel generator/src --out-dir generator/out --ignore \"src/__tests__\" --copy-files",
    "build:reason": "bsb -clean-world -make-world",
    "build:elodin": "elodin src --clean",
    "build:icons": "node scripts/icons.js && svgr -d src/icons/js src/icons/svg --icon",
    "build:storybook": "build-storybook -c .storybook -o ../website/public/storybook",
    "build:playroom": "playroom build",
    "test": "cross-env BABEL_ENV=commonjs echo 1",
    "coverage": "yarn test --coverage",
    "storybook": "start-storybook -p 6006 -s ./",
    "playroom": "playroom start",
    "reason": "bsb -clean-world -make-world -w",
    "release": "yarn build && npm publish",
    "elodin": "elodin src --watch --clean"
  },
  "license": "MIT",
  "dependencies": {
    "bs-moment": "^0.4.4",
    "bs-platform": "^6.0.x",
    "fela": "^11.0.2",
    "fela-plugin-named-keys": "^11.0.2",
    "fela-plugin-responsive-value": "^11.0.2",
    "fela-plugin-rtl": "^11.0.2",
    "fela-preset-web": "^11.0.2",
    "fela-sort-media-query-mobile-first": "^11.0.2",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dates": "^21.0.1",
    "react-dom": "^16.9.0",
    "react-fela": "^11.0.2",
    "reason-fela": "^11.0.2",
    "reason-react": "^0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@elodin/cli": "^0.0.41",
    "@elodin/core": "^0.0.41",
    "@elodin/generator-reason": "^0.0.41",
    "@elodin/parser": "^0.0.41",
    "@elodin/plugin-replace-variable": "^0.0.41",
    "@elodin/utils": "^0.0.41",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@svgr/cli": "^4.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-bucklescript": "^0.5.3",
    "cross-env": "^6.0.3",
    "jest": "^23.6.0",
    "playroom": "^0.9.4",
    "read-transform-write": "^2.1.0",
    "rimraf": "^3.0.0",
    "xmldom": "^0.1.27"
  }
}
